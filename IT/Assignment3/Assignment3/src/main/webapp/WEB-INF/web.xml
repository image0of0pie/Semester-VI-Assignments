<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>QueryFlight</servlet-name>
    <servlet-class>QueryFlight</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Deals</servlet-name>
    <servlet-class>Deals</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Info</servlet-name>
    <servlet-class>Info</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>QueryFlight</servlet-name>
    <url-pattern>/queryflight</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Deals</servlet-name>
    <url-pattern>/deals</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Info</servlet-name>
    <url-pattern>/info</url-pattern>
  </servlet-mapping>

</web-app>
